    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.NumeroContacto)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.NombreContacto)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.NombreContacto)
    </dd>
    <dt class="col-sm-2">
