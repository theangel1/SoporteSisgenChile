@model SoporteDteCore.Models.ViewModels.WorkOrderViewModel

@{
    ViewData["Title"] = "Detalles";
}

<h3 class="text-info">Detalles Orden de Trabajo</h3>

<div class="container">    
    <hr />
    <dl class="row">      
        <dt class="text-danger col-sm-2">
            Visita asociada
        </dt>
        <dd class="col-sm-10">
            @Model.VisitaData
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Empresa.RazonSocial)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Empresa.RazonSocial)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Empresa.RutEmpresa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Empresa.RutEmpresa)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.HoraLlegada)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.HoraLlegada)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.HoraSalida)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.HoraSalida)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.MotivoVisita)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.MotivoVisita)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.TrabajoRealizado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.TrabajoRealizado)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.Observaciones)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.Observaciones)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.Derivado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.Derivado)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.NombrePersonaRecibe)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.NombrePersonaRecibe)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.NombreImagenFirma)
        </dt>
        <dd class="col-sm-10">
            <img src="@Model.WorkOrder.NombreImagenFirma" />
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.NombreTecnico)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.NombreTecnico)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkOrder.IsActive)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkOrder.IsActive)
        </dd>
    </dl>
    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.WorkOrder.Id" class="btn btn-outline-warning">Editar</a>
        <a asp-action="Index" class="btn btn-outline-info">Volver</a>
    </div>
</div>

